{
  "redirects": [
    { "source": "/index.html", "destination": "/explore/indexpage", "statusCode": 301  },
    { "source": "/oho/(.)(.*)", "destination": "/explore/$1", "statusCode": 301  },
    { "source": "/wa/(.*)", "destination": "/explore/$1", "statusCode": 301  }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://hq.asodesk.com/api/$1" }
  ]
}

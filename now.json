{
  "redirects": [
    { "source": "/index.html", "destination": "/explore/indexpage", "statusCode": 301  },
    { "source": "/o/?((?!general).*)", "destination": "/explore/$1", "statusCode": 301  },
    { "source": "/wa/((?!general).*)", "destination": "/explore/$1", "statusCode": 301  }
  ],
  "rewrites": [
    { "source": "/api/((?!general).*)", "destination": "https://hq.asodesk.com/api/$1" }
  ]
}
